#include <iostream>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);

    int a, b, c, d, e, f;
    cin >> a >> b >> c >> d >> e >> f;

    // 분모 계산
    int denominator = a * e - b * d;

    int x = (c * e - b * f) / denominator;
    int y = (a * f - c * d) / denominator;

    // 정수 해 (x, y) 출력
    cout << x << " " << y << '\n';

    return 0;
}
