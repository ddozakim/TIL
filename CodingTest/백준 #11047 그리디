#include <iostream>
#include <stack>
#include <string>
#include <vector>
#include <algorithm>
using namespace std;

string explosion;
stack<char> s;

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
   
    int N,K;
    cin>>N>>K;
    vector<int> v(N);
    for(int i=0 ;i<N;i++){
        cin>>v[i];
    }
    sort(v.begin(),v.end(),greater<int>());
    
    int cnt=0;
    for(int i=0;i<N;i++){
        cnt+= K/v[i];
        K%=v[i];
    }
    
    cout<<cnt;
    
    return 0;
}
