#include <iostream>

using namespace std;


int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    
    long long S;
    int N=0;
    cin>>S;
    int num=0;
    int i=1;
    while(true){
        num+=i;
        N++;
        if(num>S){
            N--;
            break;
        }
        i++;
    }
  
    cout<<N;
    return 0;
}
