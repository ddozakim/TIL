#include<iostream>
using namespace std;

int main(){
    ios::sync_with_stdio(false);
    cin.tie(0);
    
    int test, N ,M;
    int a,b;
    cin >> test;
    while(test--){
        cin >> N >> M;
        for(int i=0; i < M ; i++){
            cin >> a >> b;
        }
        cout << N-1 <<"\n";
    }
    return 0;
}
