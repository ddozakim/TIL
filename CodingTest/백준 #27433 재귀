#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

long long factorial(int n){
    if(n==0) return 1;
    if(n==1) return 1;
    return n*factorial(n-1);
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    
    int n;
    cin>>n;
    
    cout<<factorial(n);
    
    return 0;
}
