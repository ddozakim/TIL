#include <iostream>
using namespace std;


void func(int n){
    vector<int> v ={0,1,2};
    for(int i=3; i<=n ;i++){
        v.push_back((v[i-1]+v[i-2])%15746);
    }
    cout<< v[n];
    
}


int main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    
    int n;
    cin >> n;
    
    func(n);
    return 0;
}
