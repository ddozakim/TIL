#include <iostream>
using namespace std;

string str[5];

int main()
{
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	

	for(int i=0; i<5;i++){
	    cin>>str[i];
	}
	
	for(int i=0; i<15 ; i++){
	    for(int j=0; j<5;j++){
	        if(str[j][i]!=NULL) cout<<str[j][i];
	    }
	}
	
    return 0;
}
