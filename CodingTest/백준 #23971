#include <iostream>

using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);

   int H,W,N,M;
   cin>>H>>W>>N>>M;
   
   int count=0;
   
   for(int i=0; i<H;i=i+N+1){
       for(int j=0; j<W;j=j+M+1){
           count++;
       }
   }
    
    cout<<count;
    return 0;
}
