//백준 10870

#include <iostream>
using namespace std;

int factorial(int n){
    if(n==0){
        return 1;
    }
    if(n==1){
        return 1;
    }
    
    return factorial(n-1)*n;
}

int main()
{
    ios::sync_with_stdio(false);  
    cin.tie(NULL);            
    
    int N;
    cin>>N;
    cout<<factorial(N);
    return 0;
}
